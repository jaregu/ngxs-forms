{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Karma",
      "program": "${workspaceFolder}/node_modules/karma/bin/karma",
      "args": [
        "start"
      ],
      "env": {
        "Path": "${env:Path};node_modules/.bin"
      },
      "sourceMaps": false
    }
  ]
}
